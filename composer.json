{
    "name": "danilovl/async-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle provides simple delayed function call.",
    "keywords": ["php","symfony","bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Danilov"
        }
    ],
    "autoload": {
        "psr-4": {
            "Danilovl\\AsyncBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danilovl\\AsyncBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "symfony/framework-bundle": "^6.3",
        "symfony/http-kernel": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    }
}
